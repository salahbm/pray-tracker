{
  "$schema": "https://json.schemastore.org/tsconfig",
  "extends": "./base.json",
  "compilerOptions": {
    "module": "ESNext",
    "moduleResolution": "Bundler",
    "jsx": "preserve",
    "noEmit": true,
    "plugins": [{ "name": "next" }],
    "types": ["node", "react", "react-dom"]
  },
  "include": ["**/*.ts", "**/*.tsx", "next-env.d.ts"]
}
