{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@/*": [
        "./src/*"
      ],
      "@assets/*": [
        "./src/assets/*"
      ],
      "@components/*": [
        "./src/components/*"
      ],
      "@constants/*": [
        "./src/constants/*"
      ],
      "components/*": [
        "./src/components/*"
      ],
      "constants/*": [
        "./src/constants/*"
      ],
      "@components/*": [
        "./src/components/*"
      ],
      "@constants/*": [
        "./src/constants/*"
      ],
      "components/*": [
        "./src/components/*"
      ],
      "constants/*": [
        "./src/constants/*"
      ],
      "@hooks/*": [
        "./src/hooks/*"
      ],
      "hooks/*": [
        "./src/hooks/*"
      ],
      "@lib/*": [
        "./src/lib/*"
      ],
      "lib/*": [
        "./src/lib/*"
      ],
      "@locales/*": [
        "./src/locales/*"
      ],
      "@providers/*": [
        "./src/providers/*"
      ],
      "providers/*": [
        "./src/providers/*"
      ],
      "@store/*": [
        "./src/store/*"
      ],
      "store/*": [
        "./src/store/*"
      ],
      "@styles/*": [
        "./src/styles/*"
      ],
      "styles/*": [
        "./src/styles/*"
      ],
      "@types/*": [
        "./src/types/*"
      ],
      "@utils/*": [
        "./src/utils/*"
      ],
      "utils/*": [
        "./src/utils/*"
      ],
      "types/*": [
        "./src/types/*"
      ],
      "@prayer/shared": [
        "../../packages/shared/src"
      ],
      "@prayer/shared/*": [
        "../../packages/shared/src/*"
      ],
      "@prayer/db": [
        "../../packages/db/src"
      ],
      "@prayer/db/*": [
        "../../packages/db/src/*"
      ]
    },
    "module": "esnext"
  },
  "include": [
    "./src/**/*.ts",
    "./src/**/*.tsx",
    "./src/**/*.d.ts",
    ".expo/types/**/*.ts",
    "expo-env.d.ts",
    "**/*.d.ts",
    "babel.config.js",
    "metro.config.js",
    "nativewind-env.d.ts"
  ]
}
